    "compilerOptions": {
        "outDir": "tsbuild",
        "allowSyntheticDefaultImports": true,
        "jsx": "preserve",
        "target": "es6",
        "module": "es6",
        "sourceMap": true,
        "noImplicitAny": true
    },
    "include": [
        "src/**/*.tsx",
        "src/**/*.ts",
    ]
}